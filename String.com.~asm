name "String"
org 100h

start:

mov ah, 9h
mov dx, message1
int 21h

call nextline

mov ah, 9h
mov dx, message2
int 21h

call nextline

mov ah, 9h
mov dx, message3
int 21h

call nextline

mov ah, 9h
mov dx, message4
int 21h

call nextline


jmp exit

nextline:
mov ah, 02h
mov dl, 0ah
int 21h

mov dl, 0dh
int 21h
ret

message1: db "Hello Stas$"
message2: db "I LOVE $"
message3: db "ASSEMBLY$"
message4: db "NO)))))$"

exit:
mov ax, 4c00h
int 21h

ret





